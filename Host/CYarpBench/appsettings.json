{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:7002"
      },
      "Https": {
        "Url": "https://+:7003"
      }
    }
  },
  "CYarpOptions": {
    "HttpTunnel": {
      "MaxTunnelsPerClient": 200
    }
  },
  "CYarpClientOptions": {
    "Http11Client": {
      "ServerUri": "http://localhost:7002",
      "TargetUri": "http://localhost/",
      "Authorization": "CustomDomain http11.bench.com"
    },
    "Http2Client": {
      "ServerUri": "https://localhost:7003",
      "TargetUri": "http://localhost/",
      "Authorization": "CustomDomain http2.bench.com"
    },
    "WebSocketWithHttp11Client": {
      "ServerUri": "ws://localhost:7002",
      "TargetUri": "http://localhost/",
      "Authorization": "CustomDomain websocket.bench.com"
    },
    "WebSocketWithHttp2Client": {
      "ServerUri": "wss://localhost:7003",
      "TargetUri": "http://localhost/",
      "Authorization": "CustomDomain websocketh2.bench.com"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Yarp.ReverseProxy": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Server": "Error"
    }
  },
  "AllowedHosts": "*"
}
